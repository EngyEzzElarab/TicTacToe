   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
      <children>
         <Button fx:id="b1" layoutX="135.0" layoutY="20.0" mnemonicParsing="false" onAction="#markOnGUI" prefHeight="80.0" prefWidth="92.0" style="-fx-background-color: DODGERBLUE; -fx-border-color: BLACK;" textFill="WHITE">
